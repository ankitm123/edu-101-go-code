{
    "image": "temporalio/gitpod-edu:1.3",
    "customizations": {
        "codespaces": {
            "openFiles": [
                "README.md"
            ]
        }
    },
    "forwardPorts": [8080, 7233, 9999],
    "portsAttributes": {
        "8080": {
            "onAutoForward": "openPreview"
        }
    },
    "postStartCommand": "temporal server start-dev --ui-port 8080",
    "postAttachCommand": "${command:workbench.action.terminal.split}"
}